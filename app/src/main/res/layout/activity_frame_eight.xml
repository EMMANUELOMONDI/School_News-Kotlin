<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameEightVM" type="com.emmanuelsapplication.app.modules.frameeight.data.viewmodel.FrameEightVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackplaceyourdesi"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <ImageView android:id="@+id/imagePlaceyourDesi"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_placeyourdesi_827x400}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_placeyourdesi_827x400"/>
        <LinearLayout android:id="@+id/linearColumnvector"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_37pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowvector"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_22pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_31pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_black_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEditProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_84pxh"
                        android:layout_weight="0.74"
                        android:layout_width="0dp"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{frameEightVM.frameEightModel.txtEditProfile}"
                        style="@style/txtDosissemibold20" tools:text="@string/lbl_edit_profile2"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:id="@+id/linearColumnellipsetwentyone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <FrameLayout
                    android:id="@+id/frameStackellipsetwentyone"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_200pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_200pxh">
                    <ImageView android:id="@+id/imageEllipseTwentyOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_200pxh"
                        android:layout_width="@dimen/_200pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse21}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse21"/>
                    <FrameLayout
                        android:id="@+id/frameStacksvgrepoiconcar"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_17pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_60pxh" style="@style/groupStylegray_400_e0">
                        <ImageView android:id="@+id/imageSVGRepoiconCar"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_14pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_svgrepoiconcar"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_svgrepoiconcar"/>
                    </FrameLayout>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtYoucanedityo"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_39pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{frameEightVM.frameEightModel.txtYoucanedityo}"
                    style="@style/txtDosismedium22" tools:text="@string/msg_you_can_edit_yo"/>
                <EditText
                    android:background="@drawable/rectangle_bg_gray_400_33_radius_10"
                    android:drawableEnd="@drawable/img_user_black_900"
                    android:drawablePadding="@dimen/_30pxh"
                    android:ellipsize="end"
                    android:hint="@string/lbl_username"
                    android:id="@+id/etInput" android:inputType="text"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_14pxv"
                    android:paddingEnd="@dimen/_22pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_17pxv"
                    android:singleLine="true"
                    android:text="@={frameEightVM.frameEightModel.etInputValue}"
                    android:textColorHint="@color/black_900_7f"
                    style="@style/etSolidRounded" tools:ignore="Autofill"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSave"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_33pxv"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_save"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_save"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>

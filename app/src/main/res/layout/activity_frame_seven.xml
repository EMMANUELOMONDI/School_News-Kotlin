<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="frameSevenVM" type="com.emmanuelsapplication.app.modules.frameseven.data.viewmodel.FrameSevenVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackplaceyourdesi"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_1pxh" android:layout_width="match_parent">
        <ImageView android:id="@+id/imagePlaceyourDesi"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_placeyourdesi_827x400}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_placeyourdesi_827x400"/>
        <LinearLayout android:id="@+id/linearColumnvectorOne"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearColumnlinesix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyle">
                <LinearLayout android:id="@+id/linearColumnframe"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowframe"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View android:background="@color/white_A700"
                            android:id="@+id/viewFrame"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginBottom="@dimen/_7pxv" android:layout_width="@dimen/_32pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSettings"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_110pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_weight="0.68"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{frameSevenVM.frameSevenModel.txtSettings}"
                            style="@style/txtDosissemibold20" tools:text="@string/lbl_settings"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageEllipseTwentyOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_150pxh"
                        android:layout_marginEnd="@dimen/_39pxh"
                        android:layout_marginStart="@dimen/_39pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_150pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse21}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse21"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtJohnDoe"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_59pxh"
                        android:layout_marginStart="@dimen/_59pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="wrap_content"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{frameSevenVM.frameSevenModel.txtJohnDoe}"
                        style="@style/txtDosisbold28" tools:text="@string/lbl_john_doe"/>
                    <TextView android:ellipsize="end"
                        android:gravity="end" android:id="@+id/txtEmail"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="match_parent"
                        android:letterSpacing="0.01"
                        android:singleLine="true"
                        android:text="@{frameSevenVM.frameSevenModel.txtEmail}"
                        style="@style/txtDosisregular20" tools:text="@string/msg_johndoe254_stra"/>
                </LinearLayout>
                <View android:background="@color/black_900"
                    android:id="@+id/lineLineSix"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_28pxv" android:layout_width="@dimen/_328pxh"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnclippathgroup"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_60pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowclippathgroup"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageClippathgroup"
                            android:layout_height="@dimen/_31pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_width="@dimen/_31pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_clippathgroup"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_clippathgroup"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEditprofile"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.76"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{frameSevenVM.frameSevenModel.txtEditprofile}"
                            style="@style/txtDosissemibold22" tools:text="@string/lbl_edit_profile"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowvector"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVector"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_black_900_23x23"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_black_900_23x23"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtEmailpreferenc"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_weight="0.87"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{frameSevenVM.frameSevenModel.txtEmailpreferenc}"
                            style="@style/txtDosissemibold22" tools:text="@string/msg_email_preferenc"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowarrowright"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_1pxh"
                        android:layout_marginTop="@dimen/_33pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_height="@dimen/_26pxh"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_width="@dimen/_26pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowright_red_700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_red_700"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLogout"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_weight="0.7"
                            android:layout_width="0dp"
                            android:letterSpacing="0.01"
                            android:singleLine="true"
                            android:text="@{frameSevenVM.frameSevenModel.txtLogout}"
                            style="@style/txtDosissemibold22" tools:text="@string/lbl_logout"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameBottombar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" style="@style/groupStylewhite_1">
                <LinearLayout android:id="@+id/linearRowvectorOne"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageVectorOne"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginStart="@dimen/_136pxh"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector_indigo_200"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector_indigo_200"/>
                    <LinearLayout android:id="@+id/linearColumnuser"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_width="0dp" android:orientation="vertical">
                        <ImageView android:id="@+id/imageUser"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user_blue_800"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user_blue_800"/>
                        <View
                            android:background="@drawable/rectangle_bg_blue_800_radius_2_5"
                            android:id="@+id/viewFocus"
                            android:layout_height="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_5pxh"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
